{
  "name": "json-formatter",
  "version": "1.1.0",
  "description": "Format a string containing (valid) js variables into proper JSON so it can be handled by JSON.parse",
  "main": "json-formatter.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "posttest": "./node_modules/.bin/nsp audit-package",
    "test": "./node_modules/.bin/lab -f -t 100 -m 3000",
    "test-coverage": "./node_modules/.bin/lab -f -r html -m 3000 -o report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rspieker/json-formatter.git"
  },
  "keywords": [
    "json",
    "string"
  ],
  "author": "Rogier Spieker",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/rspieker/json-formatter/issues"
  },
  "homepage": "https://github.com/rspieker/json-formatter#readme",
  "devDependencies": {
    "code": "^1.5.0",
    "coveralls": "^2.11.4",
    "lab": "^5.16.1",
    "nsp": "^1.1.0"
  }
}
